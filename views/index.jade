extends layout

block content

	script.
		// Variable parseadas por el servidor.
		var username = '#{user.name}';
		var userprofile = '#{user.perfil}';
		var region = '#{Region}';
		var urlbackupservice = '#{BackupServiceDomain}';
		var LoginTimeout = '#{LoginTimeout}';
		var version = '#{Version}';
		var bR16Mode ='#{R16Mode}';
		var bLoadIndexControlEnabled ='#{LoadIndexControlEnabled}';
		$(document).ready(function(){
			// Localizacion de las variables parseadas.
			$('#BodyRedan').data('perfil', userprofile);
			$('#BodyRedan').data('urlbackupservice', urlbackupservice);
			$('#BodyRedan').data('region',region);
			$('#BodyRedan').data('LoginTimeout',LoginTimeout);
			$('#BodyRedan').data('version',version);

			$('#BodyRedan').data('R16Mode',bR16Mode);
			$('#BodyRedan').data('LoadIndexControlEnabled',bLoadIndexControlEnabled);
			indexInitUserData(username, userprofile);
			SetupOptionsForServer(bR16Mode, bLoadIndexControlEnabled);
			});
